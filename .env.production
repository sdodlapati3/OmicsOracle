# Production Environment Overrides
# This file contains only production-specific settings
# Base configuration should be in .env.example

ENVIRONMENT=production
DEBUG=false
LOG_LEVEL=WARNING

# Ports
MAIN_PORT=8001
LEGACY_PORT=8000
HTTP_PORT=80
HTTPS_PORT=443

# Build Configuration
DOCKERFILE=Dockerfile.production
INTERNAL_PORT=8001
COMPOSE_PROFILES=production,legacy

# Database Configuration
DATABASE_SERVICE=postgres
DATABASE_URL=postgresql://omics:${POSTGRES_PASSWORD}@postgres:5432/omics_oracle

# PostgreSQL Configuration
POSTGRES_PORT=5432
POSTGRES_DB=omics_oracle
POSTGRES_USER=omics
POSTGRES_PASSWORD=${POSTGRES_PASSWORD}

# Redis Configuration
REDIS_URL=redis://redis:6379
REDIS_PORT=6379

# Security Settings
SECURE_COOKIES=true
SECURE_SSL_REDIRECT=true
CORS_ORIGINS=${PRODUCTION_DOMAIN}

# API Keys (Set via deployment system)
OPENAI_API_KEY=${OPENAI_API_KEY}
NCBI_API_KEY=${NCBI_API_KEY}
NCBI_EMAIL=${NCBI_EMAIL}
JWT_SECRET_KEY=${JWT_SECRET_KEY}
