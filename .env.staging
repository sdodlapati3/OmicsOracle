# Staging Environment Overrides
# This file contains only staging-specific settings

ENVIRONMENT=staging
DEBUG=false
LOG_LEVEL=INFO

# Ports
MAIN_PORT=8001
LEGACY_PORT=8000

# Build Configuration
DOCKERFILE=Dockerfile.production
INTERNAL_PORT=8001
COMPOSE_PROFILES=default,legacy

# Database Configuration
DATABASE_SERVICE=postgres
DATABASE_URL=postgresql://omics:staging_password_change_me@postgres:5432/omics_oracle

# PostgreSQL Configuration
POSTGRES_PORT=5432
POSTGRES_DB=omics_oracle
POSTGRES_USER=omics
POSTGRES_PASSWORD=staging_password_change_me

# Redis Configuration
REDIS_URL=redis://redis:6379
REDIS_PORT=6379

# Security Settings
JWT_SECRET_KEY=staging-secret-key-change-in-production

# API Keys (Set via deployment system)
OPENAI_API_KEY=${OPENAI_API_KEY}
NCBI_API_KEY=${NCBI_API_KEY}
NCBI_EMAIL=${NCBI_EMAIL}
