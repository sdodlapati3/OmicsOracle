{
  "name": "futuristic-interface-enhanced",
  "version": "2.0.0",
  "description": "Next-generation OmicsOracle interface with Clean Architecture integration",
  "main": "static/js/main.js",
  "scripts": {
    "dev": "concurrently \"npm run build:watch\" \"python main.py\"",
    "build": "webpack --mode production",
    "build:watch": "webpack --mode development --watch",
    "serve": "python main.py",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint static/js/ --ext .js,.ts",
    "lint:fix": "eslint static/js/ --ext .js,.ts --fix",
    "format": "prettier --write \"static/**/*.{js,ts,css,html}\"",
    "format:check": "prettier --check \"static/**/*.{js,ts,css,html}\"",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist/ node_modules/.cache/",
    "prepare": "npm run build"
  },
  "keywords": [
    "bioinformatics",
    "genomics",
    "fastapi",
    "websockets",
    "real-time",
    "visualization",
    "clean-architecture"
  ],
  "author": "OmicsOracle Team",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.2.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.6.0",
    "jest-environment-jsdom": "^29.6.0",
    "mini-css-extract-plugin": "^2.7.6",
    "prettier": "^3.0.0",
    "style-loader": "^3.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "chart.js": "^4.3.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^2.30.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "roots": ["<rootDir>/static/js", "<rootDir>/tests/frontend"],
    "testMatch": ["**/__tests__/**/*.ts", "**/?(*.)+(spec|test).ts"],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "setupFilesAfterEnv": ["<rootDir>/tests/frontend/setup.ts"],
    "collectCoverageFrom": [
      "static/js/**/*.ts",
      "!static/js/**/*.d.ts",
      "!static/js/**/index.ts"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
